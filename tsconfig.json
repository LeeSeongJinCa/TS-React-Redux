{
  "compilerOptions": {
    "outDir": "./dist",
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "target": "es5",
    "sourceMap": true,
    "baseUrl": "src",
    "typeRoots": [
      "/node_modules/@types",
      "@types"
    ]
  },
  "include": [
    "./src/**/*", "src/actions/ActionTypes.ts"
  ],
  "exclude": [
    "./node_modules"
  ]
}