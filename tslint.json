{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "no-empty-interface": true
  },
  "rulesDirectory": []
}